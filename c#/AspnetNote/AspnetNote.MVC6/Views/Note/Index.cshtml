
@{
    ViewData["Title"] = "Notice";
}

<h1>게시판</h1>
<table class="table table-dark table-striped">
  <thead>
      <thead>
    <tr>
      <th scope="col">게시물번호</th>
      <th scope="col">제목</th>
      <th scope="col">작성자</th>
    </tr>
  </thead>
  <tbody>
            @foreach(var note in Model)
            {
                
           <tr>
             <td>@note.NoteNo</td> 
             <td>@note.NoteTitle</td>       
             <td>@note.UserNo</td>        
           </tr>
            }

  </thead>
</table>

<a class="btn btn-warning" asp-controller="Note" asp-action="Add">게시물추가</a>

